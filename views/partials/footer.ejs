<script>
    $(document).ready(()=>{
        $('.time-moment').each((idx, item)=>{
            var time = moment.unix($(item).text())
            // console.log($(item).text());
            var timeHtml = `<a href="#" data-toggle="tooltip" title="${time.format('MMMM Do YYYY, h:mm:ss a')}">${time.fromNow()}</a>`;
            $(item).html(timeHtml)
        })
        $('.bignumber').each((idx, item)=>{
            var num = new BigNumber($(item).text())
            // console.log(num.toString(10));
            if($(item).data('unit') == 'eth') {
                num = num.div(1e18)
            }
            var t = num.toString(10);
            // var timeHtml = `<a href="#" data-toggle="tooltip" title="${time.format('MMMM Do YYYY, h:mm:ss a')}">${time.fromNow()}</a>`;
            $(item).text(t)
        })
    })
</script>